<p id="notice"><%= notice %></p>

<h1>Listing Connpasses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @connpasses.each do |connpass| %>
      <tr>
        <td><%= link_to 'Show', connpass %></td>
        <td><%= link_to 'Edit', edit_connpass_path(connpass) %></td>
        <td><%= link_to 'Destroy', connpass, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Connpass', new_connpass_path %>
